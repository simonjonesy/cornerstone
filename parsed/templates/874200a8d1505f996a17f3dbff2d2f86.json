{"components/cart/shipping-estimator":"<style>\r\n    .cart-total-value{\r\n        padding-top:0;\r\n        padding-bottom:0;\r\n        margin-left: auto;\r\n        text-align: right;\r\n    }\r\n    .coupon__apply{\r\n        border-radius:0;\r\n    }\r\n</style>\r\n{{#if shipping_cost}}\r\n    <div class=\"cart-total-value\">\r\n        <div class=\"subtotal shipping-estimate-show\">\r\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\r\n        </div>\r\n    </div>\r\n{{else}}\r\n    <div class=\"cart-total-value\">\r\n        <button class=\"shipping-estimate-show\">Add information</button>\r\n        <button class=\"shipping-estimate-hide\" style=\"display: none;\">Cancel</button>\r\n    </div>\r\n{{/if}}\r\n\r\n<div class=\"shipping-estimator u-hiddenVisually\">\r\n    <form class=\"form estimator-form\" data-shipping-estimator>\r\n        <dl>\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input\">\r\n                <select id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\r\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\r\n                    {{#each countries}}\r\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                            {{name}}\r\n                        </option>\r\n                    {{/each}}\r\n                </select>\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input\">\r\n                {{#if states.length}}\r\n                    <select id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\r\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\r\n                        {{#each states}}\r\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                                {{name}}\r\n                            </option>\r\n                        {{/each}}\r\n                    </select>\r\n                {{else}}\r\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\r\n                {{/if}}\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\r\n            </dd>\r\n            <button class=\"btn btn--primary button--small shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\r\n        </dl>\r\n    </form>\r\n    <div class=\"shipping-quotes\"></div>\r\n</div>\r\n","components/cart/totals":"<style>\r\n    button.coupon__apply{\r\n        border-radius:0;\r\n    }\r\n</style>\r\n    <div class=\"order-summary\">\r\n        <h3>Order Summary</h3>\r\n        <ul class=\"order-summary__details\">\r\n            <li>\r\n                <span>{{lang 'cart.checkout.subtotal'}}:</span>\r\n                <span>{{cart.sub_total.formatted}}</span>\r\n            </li>\r\n            {{#if cart.shipping_handling.show_estimator}}\r\n            <li>\r\n                <span>{{lang 'cart.checkout.shipping'}}</span>\r\n                {{> components/cart/shipping-estimator cart.shipping_handling}}\r\n            </li>\r\n            {{/if}}\r\n            {{#each cart.taxes}}\r\n            <li>\r\n                <span>{{name}}</span>\r\n                <span>{{cost.formatted}}</span>\r\n            </li>\r\n            {{/each}}\r\n           <!-- {{#if currency_selector.active_currency_code '!=' 'USD'}}-->\r\n            <li>\r\n                <span>INCLUDES GST (AUS)</span>\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                    <span>{{cart.grand_total.formatted}}</span>\r\n                {{else}}\r\n                    {{> components/common/login-for-pricing}}\r\n                {{/or}}\r\n            </li>\r\n          <!--  {{/if}} -->\r\n        </ul>\r\n        {{#if cart.coupons}}\r\n            {{#each cart.coupons}}\r\n            <div class=\"coupon\">\r\n                <div class=\"cart-total-label\">\r\n                        <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\r\n                        <div>\r\n                            <a data-url=\"{{remove_url}}\" class=\"remove-coupon\">{{lang 'cart.coupons.remove'}}</a>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"cart-total-value\">\r\n                        <span>{{discount.formatted}}</span>\r\n                    </div>\r\n            </div>\r\n            {{/each}}\r\n        {{else}}\r\n        <div class=\"coupon\">\r\n            <!--<input type=\"text\" class=\"coupon__input\" placeholder=\"Add coupon\">\r\n            <button class=\"coupon__apply\" type=\"button\">Apply</button>-->\r\n            \r\n            <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"/cart.php\">\r\n                <input class=\"coupon__input\" data-error=\"Please enter your coupon code.\" id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"Add Coupon\">\r\n                <!--<input class=\"button button--primary button--small\" type=\"submit\" value=\"Apply\">-->\r\n                <button class=\"coupon__apply\" type=\"submit\">Apply</button>\r\n                <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\r\n            </form>\r\n        </div>\r\n        {{/if}}\r\n\r\n        <div class=\"total-summary\">\r\n            <h3>Order Summary</h3>\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                <span>{{cart.grand_total.formatted}}</span>\r\n            {{else}}\r\n                {{> components/common/login-for-pricing}}\r\n            {{/or}}\r\n        </div>\r\n\r\n        <a href=\"/checkout\" class=\"btn btn--primary btn--expand\">Go to checkout</a>\r\n    </div>\r\n\r\n\r\n\r\n\r\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>\r\n"}
