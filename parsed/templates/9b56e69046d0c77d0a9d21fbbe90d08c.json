{"components/common/requireness-msg":"<small>\r\n    {{#if required}}{{lang 'common.required'}}{{else}}{{lang 'common.optional'}}{{/if}}\r\n</small>\r\n","components/products/options/input-file":"<div class=\"form-field\" data-product-attribute=\"input-file\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_file_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n\r\n    <input class=\"form-file\" type=\"file\" id=\"attribute_file_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\r\n    {{#if original_name}}\r\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\r\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\r\n    {{/if}}\r\n    <p class=\"form-fileDescription\">\r\n        {{#if file_size}}\r\n            {{lang 'products.max_filesize'}}\r\n            <strong>{{file_size}}{{lang 'products.kilobytes_abbreviation'}}</strong>,\r\n        {{/if}}\r\n        {{#if file_types}}\r\n            {{lang 'products.filetypes'}} <strong>{{file_types}}</strong>\r\n        {{/if}}\r\n    </p>\r\n\r\n</div>\r\n"}
